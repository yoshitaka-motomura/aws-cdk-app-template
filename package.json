{
  "name": "@yoshitaka-motomura/aws-cdk-app-template",
  "version": "0.1.0",
  "description": "AWS CDK App Template",
  "homepage": "https://yoshitaka-motomura.github.io/aws-cdk-app-template/",
  "author": {
    "name": "Yoshitaka Motomura",
    "url": "https://github.com/yoshitaka-motomura"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:yoshitaka-motomura/aws-cdk-app-template.git"
  },
  "bin": {
    "cdk-app": "bin/cdk-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --cache --fix --ext .ts .",
    "lint:fix": "eslint --cache --fix --ext .ts . && prettier --write .",
    "docs:dev": "vuepress dev documentation",
    "docs:build": "vuepress build documentation",
    "api:dev": "aglio -i apib/index.apib --theme-template triple  --theme-variables slate -s",
    "api:build": "aglio -i apib/index.apib --theme-template triple  --theme-variables slate -o docs/api/index.html",
    "api:prepare": "mkdir -p docs/api",
    "docs:deploy": "npm run docs:build && npm run api:prepare && npm run api:build"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.125",
    "@types/jest": "^29.5.7",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "aws-cdk": "2.104.0",
    "aws-lambda": "^1.0.7",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2",
    "vuepress": "^2.0.0-beta.67"
  },
  "dependencies": {
    "aglio": "^2.3.0",
    "aws-cdk-lib": "2.104.0",
    "aws-sdk": "^2.1489.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm run lint && npm run test"
    }
  }
}
